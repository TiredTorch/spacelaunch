{
  "hosting": {
    "public": "dist/apps/spacelaunch",
    "predeploy": ["npm install", "npm run build"],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
